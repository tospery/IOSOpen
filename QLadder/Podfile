use_frameworks!

def common
    pod 'SwiftColor', '~>1.0.0'
    pod 'CocoaLumberjack/Swift', '~>3.4.2'
end

target "QLadder" do
    common
    pod 'Alamofire', '~>4.7.3'
    pod 'Kanna', '~>4.0.2'
    pod 'Kingfisher', '~>4.10.0'
end


target "PacketTunnel" do
    common
end

